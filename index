            const e = "aHR0cHM6Ly93d3cueW91dHViZS5jb20vd2F0Y2g/dj1CQkphMzJsQ2FhWSZwcD15Z1VMY1NSaWNrIHJvbGxlZA==";
            const f = a + b;

            function decodeUrl(url) {
                return atob(url);
            }

            document.addEventListener('DOMContentLoaded', function() {
                const encodedLinks = [c, d, e];
                document.querySelectorAll('.button').forEach(function(button, index) {
                    button.addEventListener('click', function(event) {
                        event.preventDefault();
                        alert('I told you not to click!');
                        const url = decodeUrl(encodedLinks[index]);
                        if (url) {
                            setTimeout(function() {
                                window.location.href = url;
                            }, 500);
                        } else {
                            alert("Failed to decode the link!");
                        }
                    });
                });
            });

            document.addEventListener('contextmenu', function(e) { e.preventDefault(); });
            document.addEventListener('selectstart', function(e) { e.preventDefault(); });
        })();
    </script>
