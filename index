    <script>
        (function() {
            // Obfuscation by variable replacement and nesting
            const a = "YXQ";
            const b = "b2I";
            const c = "aHR0cHM6Ly93d3cueW91dHViZS5jb20vd2F0Y2g/dj1CQkphMzJsQ2FhWSZwcD15Z1VMY1NSaWNrIHJvbGxlZA==";
